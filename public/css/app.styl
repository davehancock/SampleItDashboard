
h1
  color: #2E6492

// Nav Bar Header styling
.navbar-default
  background-color: #F4F6F7
  border-top: none
  border-left: none
  border-right: none
  border-bottom: solid
  border-color: #D4D2D2
  border-width: 1px
  border-radius: 0

// Used for collapsing nav bar on resize
@media (max-width: 767px) {
  .visible-xs.collapse.in {
    display: block!important;
  }
  .visible-xs.collapse {
    display: none!important;
  }
}

.jumbotron
  background-color: #FFE3B8

// Button color override
.btn-primary
  color: #fff
  background-color: #E4841E
  border-color: #403507

.btn-primary:active
  color: #fff
  background-color: #905928
  border-color: #403507

.btn-primary:hover
  color: #fff
  background-color: #905928
  border-color: #403507

// CPU page styling
.cpu-canvas
  width: 100%
  height: 100%

.cpu-isolation-block
  float: left
  position: relative
  width: 50%

.cpu-slider-group
  margin-right: 20px

.cpu-slider
  height: 100%

.host-tabs
  margin-top: 20px
  margin-bottom: 20px

// Footer Styling
html
  position: relative
  min-height: 100%

body
  margin-bottom: 100px

.footer-content
  margin-top: 20px;
  text-align: right;

.footer
  border-top: solid
  border-color #D4D2D2
  border-width: 1px

  position: absolute
  bottom: 0
  width: 100%
  height: 60px
  background-color: #B5B5B5
  font-style: italic

:focus{
  outline: none !important;
}